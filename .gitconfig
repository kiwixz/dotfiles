[advice]
    detachedHead = false
[alias]
    cerge = merge --no-ff
    bog = log --oneline --graph --decorate=short
    bull = !"f() { git fetch origin $@:$@; }; f"
    up = !"f() { git fetch upstream $@:$@; }; f"
    pog = log --all --oneline --graph --decorate=short
[core]
    autocrlf = input
    editor = nano
[fetch]
    prune = true
[mergetool]
    keepBackup = false
[pull]
    rebase = true
